{
  "title": "Kontur Game Stats Server",
  "version": "1.0.0-alpha1",
  "copyright": "Konstantinov O. V.",
  "description": "Collects statistics from the game servers and provides a REST interface for their control.",
  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.0.1",
    "Microsoft.AspNetCore.Mvc": "1.0.2",
    "Microsoft.AspNetCore.Routing": "1.0.2",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.2",
    "Microsoft.EntityFrameworkCore.Sqlite": "1.0.2",
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-final",
    "Microsoft.Extensions.CommandLineUtils": "1.0.1",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.1",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.0.1",
    "Microsoft.Extensions.Configuration.Json": "1.0.1",
    "Microsoft.Extensions.Logging": "1.0.1",
    "Microsoft.Extensions.Logging.Console": "1.0.1",
    "Microsoft.Extensions.Logging.Debug": "1.0.1",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.1",
    "Microsoft.NETCore.App": "1.0.1",
    "Serilog": "2.4.0",
    "Serilog.Extensions.Logging": "1.4.0",
    "Serilog.Settings.Configuration": "2.2.0",
    "Serilog.Sinks.ColoredConsole": "2.0.0",
    "Serilog.Sinks.Console": "2.1.0",
    "Serilog.Sinks.File": "3.2.0",
    "Serilog.Sinks.Literate": "2.1.0",
    "Serilog.Sinks.RollingFile": "3.3.0",
    "Serilog.Sinks.RollingFileAlternate": "2.0.7"
  },

  "tools": {
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-final",
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": "appsettings.json"
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "publishOptions": "appsettings.json",

  "runtimes": {
    "win7-x86": {}
  }
}
